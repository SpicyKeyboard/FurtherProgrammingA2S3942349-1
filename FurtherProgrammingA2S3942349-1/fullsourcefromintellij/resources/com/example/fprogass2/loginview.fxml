<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="253.0" prefWidth="358.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.fprogass2.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

          <Label alignment="CENTER" text="Enter Your Login Details" />
   <HBox alignment="CENTER" prefHeight="18.0" prefWidth="318.0">
      <children>
         <Label prefHeight="42.0" prefWidth="126.0" text="Username:" />
         <TextField fx:id="textusername" accessibleRole="TEXT" prefHeight="0.0" prefWidth="194.0" promptText="Enter your username here" />
      </children>
   </HBox>
   <HBox alignment="CENTER" layoutX="30.0" layoutY="137.0" prefHeight="18.0" prefWidth="318.0">
      <children>
         <Label prefHeight="42.0" prefWidth="126.0" text="Password:" />
         <TextField fx:id="textpassword" accessibleRole="PASSWORD_FIELD" prefHeight="0.0" prefWidth="194.0" promptText="Enter your password here" />
      </children>
   </HBox>
   <Label fx:id="signinerrorlabel" prefHeight="17.0" prefWidth="225.0" textFill="RED" />
   <Button fx:id="signinbutton" alignment="CENTER" mnemonicParsing="false" onAction="#onSignInClicked" text="Sign In" />
   <Label alignment="CENTER" text="If you don't have an account you can:">
      <font>
         <Font name="System Italic" size="9.0" />
      </font>
   </Label>
   <Button fx:id="signupbutton" mnemonicParsing="false" onAction="#onSignUpClicked" text="Sign Up" />
</VBox>
